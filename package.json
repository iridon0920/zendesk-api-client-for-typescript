{
  "name": "zendesk-api-client-typescript",
  "version": "1.1.1",
  "description": "Type-safe Zendesk API client for TypeScript with automatic rate limiting",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/zendesk-api-client-typescript.git"
  },
  "homepage": "https://github.com/your-username/zendesk-api-client-typescript#readme",
  "bugs": {
    "url": "https://github.com/your-username/zendesk-api-client-typescript/issues"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build && npm run test:unit",
    "clean": "rm -rf dist",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "prepare": "npm run build",
    "prepack": "npm run clean && npm run build"
  },
  "keywords": [
    "zendesk",
    "api",
    "client",
    "typescript",
    "rest",
    "rate-limiting",
    "support",
    "helpdesk",
    "tickets",
    "users",
    "organizations"
  ],
  "author": "Claude Assistant",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
